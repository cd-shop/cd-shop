<div class="sign_in_form">
    <div class="sign_in_content">
    <h2>Log in</h2><br><br>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

      <div class="field">
        <%= f.label :email %><br/>
        <%= f.email_field :email, autofocus: true, autocomplete: "email", :placeholder => "例）aaa@gmail.com" %>
      </div>

      <div class="field">
        <%= f.label :パスワード %><br />
        <%= f.password_field :password, autocomplete: "current-password", :placeholder => "パスワードを入力してください" %>
      </div>

      <% if devise_mapping.rememberable? %>
        <div class="field">
          <%= f.check_box :remember_me %>
          <%= f.label :ユーザ情報を保存 %>
        </div><br>
      <% end %>

      <div class="actions">
        <%= f.submit "ログイン",class:"btn btn-success" %>
      </div><br>
    <% end %>
  <br>
    <%= render "devise/shared/links" %>
  </div>
</div>

<style>
    .sign_in_form {
    margin: 50px auto 50px auto;
    text-align: center;
    border :inset;
    width: 800px;
    height: 600px
  }
    .sign_in_content {
      padding-top: 40px;
    }
</style>
